{
  "name": "ts-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc && tsoa spec-and-routes",
    "start": "node dist/app.js",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "tsoa:generate": "tsoa spec-and-routes"
  },
  "dependencies": {
    "@azure/identity": "^4.10.0",
    "@azure/storage-blob": "^12.18.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "envalid": "^8.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "stripe": "^14.0.0",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.6.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.0.0",
    "@types/swagger-ui-express": "^4.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^30.2.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.0"
  }
}
